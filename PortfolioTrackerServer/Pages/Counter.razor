@page "/counter"
@using PortfolioTrackerServer.Services.GetStockInfoService;
@using System.Diagnostics;
@inject IGetStockInfoService GetStock

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

@code {
    private decimal currentCount = 0;

    private async Task IncrementCount()
    {
        var s = await GetStock.GetStockData("GOOG");
        Debug.WriteLine(s.Data.High);
        currentCount = s.Data.High;
    }
}

